<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<!-- topbar 1 start -->
	<template id="sh_corpomate_theme_style_top_bar_1" name="Top Bar Style 1" inherit_id="website.layout" active="False">
		<xpath expr="//header" position="before">
				<section id="sh_top_bar_1">
					<div class="container-fluid">
						<div class="sh_topbar_content">
							<div class="sh_tb_left">
								<div class="sh_main_content">
									<div class="sh_icon">
										<span class="fa fa-tachometer"></span>
									</div>
									<div class="sh_text">
										Raise your website traffic with invess
										<span>Free Site Analysis</span>
									</div>
								</div>
							</div>
							<div class="sh_tb_right">
								<div class="row">
									<div class="col-md-6">
										<div class="sh_contact_details bdr">
											<div class="sh_icon">
												<span class=" fa fa-phone"></span>
											</div>
											<div class="sh_content">
												<a href="tel:+917984575681">	+91 <span style="padding-left:5px;">7984575681 </span></a>
											</div>
										</div>
									</div>
									<div class="col-md-6">
										<div class="sh_contact_details">
											<div class="sh_icon">
												<span class=" fa fa-envelope"></span>
											</div>
											<div class="sh_content">
												<a href="mailto:info@softhealer.com">	info@softhealer.com </a>
											</div>
										</div>
									</div>
								</div>

							</div>
						</div>
					</div>
				</section>
			<!-- End your code -->
		</xpath>
	</template>
	<!-- topbar 1 end -->

	<!-- topbar 2 start-->
	<template id="sh_corpomate_theme_style_top_bar_2" name="Top Bar Style 2" inherit_id="website.layout" active="False">
		<xpath expr="//header" position="before">
			<section id="sh_top_bar_2">
				<div class="container">
					<div class="sh_topbar_content">
						<div class="sh_logo">
							<a class="navbar-brand" href="/">
								<img t-att-src="image_data_uri(website.logo)" id="logo-footer" class="img-fluid" alt="logo icon" style="" />
							</a>
						</div>
						<div class="sh_info">
							<div class="sh_call_info">
								<div class="sh_icon">
									<span class="fa fa-phone"></span>
								</div>
								<div class="sh_callus">
									<h4>call us on:</h4>
									<p>
										<!-- <a t-att-href="'tel: %s' %(res_company.phone)">
											<t t-esc="res_company.phone" />
										</a> -->
										<span>
											<a href="tel:+1 555-555-5556">+1 555-555-5556</a>
										</span>
									</p>
								</div>
							</div>

							<div class="sh_call_info">
								<div class="sh_icon">
									<span class="fa fa-envelope"></span>
								</div>
								<div class="sh_callus">
									<h4>mail us on:</h4>
									<p>
										<!-- <a t-att-href="'mailto: %s' %(res_company.email)">
											<t t-esc="res_company.email" />
										</a> -->
										<span>
											<a href="mailto:info@yourcompany.com">info@yourcompany.com</a>
										</span>
									</p>
								</div>
							</div>

						</div>
					</div>
				</div>
			</section>
		</xpath>
	</template>
	<!-- topbar 2 end -->

	<!-- topbar 3 start-->
