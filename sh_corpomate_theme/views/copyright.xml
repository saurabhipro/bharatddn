<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<template id="sh_corpomate_theme_style_copyright_1" name="Copyright 1" inherit_id="website.layout" active="False">
		<xpath expr="//footer" position="inside">
		  <section id="sh_copyright_1">
			<div class="container">
			  <div class="row">
				<div class="col-lg-6 col-md-6 col-sm-12 col-12 text-left font-arial sm-text-left">
				  <p class="muted">
					<span>Copyright © 2025</span>
					<a t-att-href="'%s' %(res_company.website)" target="_blank" style="color:#212121;font-weight:600">
					  <t t-esc="res_company.name" />
					</a>
					<!-- LANGUAGE SELECTOR -->
					<t t-set="flags" t-value="True" />
					<t t-call="website.language_selector">
						<t t-set="_div_classes" t-value="(_div_classes or '') + ' dropup'" />
					</t>
				  </p>
				  
				</div>
				<div class="col-lg-6 col-md-6 col-sm-12 col-12">
				  <div class="m-t-10 ">
					<ul class="no-style fs-11 no-padding font-arial">
					  <li class="inline no-padding">
						<a href="/" class="link  p-l-10 p-r-10">Privacy Policy</a>
					  </li>
					  <li class="inline no-padding">
						<a href="/" class="link  p-l-10 p-r-10">Contact</a>
					  </li>
					  <li class="inline no-padding">
						<a href="/" class="link  p-l-10 p-r-10 xs-no-padding xs-m-t-10">Help</a>
					  </li>
					</ul>
				  </div>
				</div>
			  </div>
			</div>
		  </section>
		</xpath>
	  </template>

