<?xml version="1.0" encoding="utf-8"?>
<odoo>

  <!-- CONTACT US PAGE 2 -->
  <template id="sh_corpomate_tmpl_contact_us_2" name="contact us">
    <t t-call="website.layout">
      <div id="wrap" class="oe_structure" />
      <section id="sh_corpomate_theme_industry_17" class="sh_animation_part oe_img_bg o_bg_img_center o_colored_level" data-snippet="sh_corpomate_theme_tmpl_industry_17" data-name="contact banner" style="background-image: url('/sh_corpomate_theme/static/src/images/theme_2/sh_industry_image_5.jpg');">
        <div class="container">
          <div class="sh_breacrumb">
            <a href="/home2" class="o_default_snippet_text">home</a>
            <span class="o_default_snippet_text">/</span>
            <p class="o_default_snippet_text">contact</p>
          </div>
          <h4 class="o_default_snippet_text">contact</h4>
        </div>
        <svg viewbox="0 0 100 100" preserveaspectratio="none">
          <polygon fill="white" points="0,100 100,0 100,100" />
        </svg>
      </section>
      <section id="sh_corpomate_theme_industry_18" class="sh_animation_part o_colored_level" data-snippet="sh_corpomate_theme_tmpl_industry_18" data-name="contact snippet 1" style="background-image: none;">
        <div class="container">
          <div class="sh_address">
            <div class="sh_map_address sh_animation_part">
              <div class="sh_icon">
                <span class="fa fa-map-marker" />
              </div>
              <h4 class="o_default_snippet_text">office location</h4>
              <p class="o_default_snippet_text">905, Corpomate Levels, 150 ft. Ring Road Rajkot.</p>
            </div>
            <div class="sh_map_address sh_animation_part">
              <div class="sh_icon">
                <span class="fa fa-phone" />
              </div>
              <h4 class="o_default_snippet_text">phone number</h4>
              <p>
                <a href="tel: +1 (650) 555-0111" class="o_default_snippet_text">
                    +1 (650) 555-0111 
                </a>
              </p>
            </div>
            <div class="sh_map_address sh_animation_part">
              <div class="sh_icon">
                <span class="fa fa-envelope" />
              </div>
              <h4 class="o_default_snippet_text">E-mail Address</h4>
              <p>
                <a href="mailto: info@yourcompany.com" class="o_default_snippet_text">
                    info@yourcompany.com
                </a>
              </p>
            </div>
          </div>
        </div>
      </section>
      <section id="sh_corpomate_theme_industry_19" class="sh_animation_part oe_img_bg o_bg_img_center o_colored_level" data-snippet="sh_corpomate_theme_tmpl_industry_19" data-name="contact form" style="background-image: url(&quot;/sh_corpomate_theme/static/src/images/theme_2/sh_industry_image_14.webp&quot;);">
        <div class="container">
          <section class="s_website_form" data-vcss="001" data-snippet="s_website_form">
            <div class="container">
              <form id="contactus_form" action="/website/form/" method="post" enctype="multipart/form-data" class="o_mark_required" data-mark="*" data-model_name="mail.mail" data-success-mode="redirect" data-success-page="/contactus-thank-you" data-pre-fill="true">
                <div class="s_website_form_rows row s_col_no_bgcolor">
                  <div class="form-group col-12 s_website_form_field s_website_form_custom s_website_form_required" data-type="char" data-name="Field">
                    <div class="row s_col_no_resize s_col_no_bgcolor">
                      <label class="d-none col-form-label col-sm-auto s_website_form_label" style="width: 200px" for="contact1">
                        <span class="s_website_form_label_content" />
                      </label>
                      <div class="col-md-6 col-12">
                        <input id="contact1" type="text" class="form-control s_website_form_input" name="name" placeholder="Name" required="" />
                      </div>
                      <div class="col-md-6 col-12">
                        <input id="contact2" type="tel" class="form-control s_website_form_input" placeholder="Phone" name="phone" />
                      </div>
                    </div>
                  </div>
                  <div class="form-group col-12 s_website_form_field s_website_form_required" data-type="email" data-name="Field">
                    <div class="row s_col_no_resize s_col_no_bgcolor">
                      <label class="d-none col-form-label col-sm-auto s_website_form_label" style="width: 200px" for="contact1">
                        <span class="s_website_form_label_content" />
                      </label>
                      <div class="col-md-6 col-12">
                        <input id="contact3" type="email" class="form-control s_website_form_input" name="email_from" placeholder="Email" required="" />
                      </div>
                      <div class="col-md-6 col-12">
                        <input id="contact5" type="text" class="form-control s_website_form_input" name="subject" placeholder="Subject" required="" />
                      </div>
                    </div>
                  </div>
                  <div class="form-group col-12 s_website_form_field s_website_form_custom s_website_form_required" data-type="text" data-name="Field">
                    <div class="row s_col_no_resize s_col_no_bgcolor">
                      <label class="d-none col-form-label col-sm-auto s_website_form_label" style="width: 200px" for="contact1">
                        <span class="s_website_form_label_content" />
                      </label>
                      <div class="col-sm">
                        <textarea id="contact6" class="form-control s_website_form_input" name="description" placeholder="Message.." required="" />
                      </div>
                    </div>
                  </div>
                  <div class="s_website_form_field mb-3 col-12    s_website_form_dnone" data-name="Field">
                    <div class="row s_col_no_resize s_col_no_bgcolor">
                      <label class="col-form-label col-sm-auto s_website_form_label " style="width: 200px">
                        <span class="s_website_form_label_content" />
                      </label>
                      <div class="col-sm">
                        <input type="hidden" class="form-control s_website_form_input" name="email_to" value="info@yourcompany.example.com" />
                        <div class="s_website_form_field_description small form-text text-muted">
            Separate email addresses with a comma.
        </div>
                      </div>
                    </div>
                  </div>
                  <div class="form-group col-12 s_website_form_submit" data-name="Submit Button">
                    <div class="s_website_form_label" />
                    <a href="#" role="button" class="btn btn-primary btn-lg s_website_form_send bnt_submit o_default_snippet_text">Send Message</a>
                    <span id="s_website_form_result" />
                  </div>
                </div>
              </form>
            </div>
          </section>
        </div>
      </section>
      <section id="sh_corpomate_theme_industry_20" class="sh_animation_part o_colored_level" data-snippet="sh_corpomate_theme_tmpl_industry_20" data-name="contact map" style="background-image: none;">
        <div class="container">
          <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d14764.162888844217!2d70.76170147993821!3d22.31429961505925!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x3959ca18fbb8df83%3A0xc5d2508a38a14110!2sSofthealer%20Technologies!5e0!3m2!1sen!2sin!4v1608729855171!5m2!1sen!2sin" height="450" frameborder="0" style="border:0;" allowfullscreen="" aria-hidden="false" tabindex="0" />
          <div style="position: absolute; width: 0px; height: 450px; top: 0px; left: 0px;" />
        </div>
      </section>
    </t>
  </template>
</odoo>