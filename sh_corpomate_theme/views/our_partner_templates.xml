<?xml version="1.0" encoding="utf-8"?>
<odoo>
  

  

  <!-- OUR PARTNER WITH CATEGORY SELECTION -->



  
    <!-- Snippet 1 templ-->
  <template id="sh_our_partner_with_categ_item_1" name="Our Partners">
    <div class="owl-carousel owl-theme">
      <t t-if="sh_our_partners">
        <t t-foreach="sh_our_partners" t-as="sh_partner">
          <div class="item" t-if='sh_partner and sh_partner.image'>  
            <img t-att-src="image_data_uri(sh_partner.image)"></img>
          </div>
        </t>
      </t>
    </div>
  </template>

    <!-- Snippet 1 main-->
  <template id="sh_our_partner_snippet_with_category_tmpl_demo" name="Our Partners">
    <section id="sh_our_partner_with_categ_section_1" class="js_cls_our_partners_with_category_selection" data-item_template="sh_corpomate_theme.sh_our_partner_with_categ_item_1" data-owl_item_mobile="1" data-owl_item_tablet="3" data-owl_item_desktop="5">
        <div class="container">
            <h1>Our Partners</h1>
            <div class="js_cls_our_partner_category_render_area o_not_editable" />
        </div>
    </section>
</template>
  




