<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- website form view -->
    <record id="sh_website_form_view_inherit" model="ir.ui.view">
        <field name="name">sh.website.store.locator.website.form.view.inherit</field>
        <field name="model">website</field>
        <field name="inherit_id" ref="website.view_website_form"/>
        <field name="arch" type="xml">
            <field name='language_ids' position='after'>
                <field name='google_maps_api_key'/>
                <field name='sh_map_zoom'/>
                <field name='sh_store_locator_design_type'/>
            </field>
        </field>
    </record>
</odoo>