<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<template id="sh_image_hotspot" name="Image Hotspot">
		<section class="sh_image_hotspot position-relative">
			<img class="img img-fluid" src="/sh_corpomate_theme/static/src/images/extra_addons/sh_image_hotspot_snippet/new_image_5.png"></img>
		</section>
	</template>
	
	<template id="sh_website_megamenu_design_popover_tmpl" name="Design Popover">
		<t t-call="website.layout">
			<div id="wrap" t-ignore="True">
				<hr />
				<div class="container">
					<div t-if="record">
						<div t-field="record.description" />
					</div>
				</div>
			</div>
		</t>
	</template>
	
	
</odoo>