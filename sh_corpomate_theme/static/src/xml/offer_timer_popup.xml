<?xml version="1.0" encoding="UTF-8"?>
<templates id="sh_corpomate_theme_templates" xml:space="preserve">


  <t t-name="sh_corpomate_theme.sh_popup_model_offer_timer_config">
  
  
	<div class="modal" style="z-index: 9999;" tabindex="-1" role="dialog" id="js_id_model_sh_corpomate_offer_config" data-backdrop="static" data-keyboard="false">
	  <div class="modal-dialog" style="max-width:750px;">
	    <div class="modal-content">
	      <div class="modal-header">
	        <h5 class="modal-title">Offer Time</h5>
	        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
	          <span aria-hidden="true">X</span>
	        </button>
	      </div>
	      <div class="modal-body" style="padding-top:0px;padding-bottom:0px;">

		      <form>
				<div class="container">

		            <div class="row">
		                <div class="col-md-6 col-12">
		                    <div class="form-group">
		                        <label for="start_date">Start Date:</label>
		                        <input type="date" class="form-control" name="date_start" required=""/>
		                    </div>
		                </div>
		                <div class="col-md-6 col-12">
		                   <div class="form-group">
		                       <label for="end_date">End Date:</label>
		                       <input type="date" class="form-control" name="date_end" required=""/>
		                    </div>
		                </div>
		            </div>

				</div>		      
		     </form>

	      </div>
	      
	      <div class="modal-footer">
	        <button type="button" class="btn btn-primary js_cls_btn_apply_sh_offer_timer_config">Done</button>
	        <button type="button" class="btn btn-secondary js_cls_btn_close_sh_offer_timer_config" data-dismiss="modal">Close</button>
	      </div>
	    </div>
	  </div>
	</div>


    
  </t>
  
  
  
</templates>